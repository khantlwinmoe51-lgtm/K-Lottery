                            <div class="lottery-ball purple w-6 h-6 rounded-full flex items-center justify-center text-xs font-bold">4</div>
                            <div class="lottery-ball blue w-6 h-6 rounded-full flex items-center justify-center text-xs font-bold">6</div>
                            <div class="lottery-ball orange w-6 h-6 rounded-full flex items-center justify-center text-xs font-bold">0</div>
                        </div>
                    </div>
                    <span class="text-xs text-gray-500">2 min ago</span>
                </div>
                
                <div class="flex items-center justify-between p-3 bg-gray-50 rounded-xl">
                    <div class="flex items-center space-x-2">

<span class="text-sm text-gray-600">#1245</span>
                        <div class="flex space-x-1">
                            <div class="lottery-ball red w-6 h-6 rounded-full flex items-center justify-center text-xs font-bold">1</div>
                            <div class="lottery-ball green w-6 h-6 rounded-full flex items-center justify-center text-xs font-bold">7</div>
                            <div class="lottery-ball blue w-6 h-6 rounded-full flex items-center justify-center text-xs font-bold">3</div>
                            <div class="lottery-ball purple w-6 h-6 rounded-full flex items-center justify-center text-xs font-bold">9</div>
                            <div class="lottery-ball yellow w-6 h-6 rounded-full flex items-center justify-center text-xs font-bold">5</div>
                        </div>
                    </div>
                    <span class="text-xs text-gray-500">5 min ago</span>
                </div>
                
                <div class="flex items-center justify-between p-3 bg-gray-50 rounded-xl">
                    <div class="flex items-center space-x-2">
                        <span class="text-sm text-gray-600">#1244</span>
                        <div class="flex space-x-1">
                            <div class="lottery-ball purple w-6 h-6 rounded-full flex items-center justify-center text-xs font-bold">4</div>
                            <div class="lottery-ball orange w-6 h-6 rounded-full flex items-center justify-center text-xs font-bold">2</div>
                            <div class="lottery-ball green w-6 h-6 rounded-full flex items-center justify-center text-xs font-bold">8</div>
                            <div class="lottery-ball red w-6 h-6 rounded-full flex items-center justify-center text-xs font-bold">1</div>
                            <div class="lottery-ball blue w-6 h-6 rounded-full flex items-center justify-center text-xs font-bold">6</div>
                        </div>
                    </div>
                    <span class="text-xs text-gray-500">8 min ago</span>
                </div>
            </div>
        </div>
    </div>

    <script>
        let selectedChoice = null; // 'big', 'small', 'green', or 'purple'
        let selectedMultiplier = 1;
        let baseBet = 1000; // 1000 kyats base bet
        let countdownTimer = 30;
        let timerInterval;

        // Initialize timer
        function startTimer() {
            clearInterval(timerInterval);
            timerInterval = setInterval(() => {
                countdownTimer--;
                updateTimerDisplay();
                
                if (countdownTimer <= 0) {
                    countdownTimer = 30; // Reset to 30 seconds
                    simulateNewDraw();
                }
            }, 1000);
        }

        function updateTimerDisplay() {
            const minutes = Math.floor(countdownTimer / 60);
            const seconds = countdownTimer % 60;
            const display = ${minutes.toString().padStart(2, '0')}:${seconds.toString().padStart(2, '0')};
            document.getElementById('timer-text').textContent = display;
        }

        function setTimer(seconds) {
            countdownTimer = seconds;
            updateTimerDisplay();
            
            // Visual feedback for timer selection
            document.querySelectorAll('.timer-badge').forEach(badge => {
                badge.classList.remove('bg-red-600');
                badge.classList.add('bg-gray-800');
            });
            event.target.classList.remove('bg-gray-800');
            event.target.classList.add('bg-red-600');
        }

        function selectBigSmall(choice) {
            // Remove previous selection
            document.querySelectorAll('.big-small-btn').forEach(btn => {

btn.classList.remove('selected');
            });
            
            // Add selection to clicked button
            if (choice === 'big') {
                document.getElementById('big-btn').classList.add('selected');
                selectedChoice = 'big';
            } else {
                document.getElementById('small-btn').classList.add('selected');
                selectedChoice = 'small';
            }
            
            updateSelectionDisplay();
            updateBetAmount();
        }

        function selectColor(color) {
            // Remove previous selection
            document.querySelectorAll('.big-small-btn').forEach(btn => {
                btn.classList.remove('selected');
            });
            
            // Add selection to clicked button
            if (color === 'red') {
                document.getElementById('red-btn').classList.add('selected');
                document.getElementById('red-btn-timer').classList.add('selected');
                selectedChoice = 'red';
            } else if (color === 'green') {
                document.getElementById('green-btn').classList.add('selected');
                document.getElementById('green-btn-timer').classList.add('selected');
                selectedChoice = 'green';
            } else if (color === 'purple') {
                document.getElementById('purple-btn').classList.add('selected');
                document.getElementById('purple-btn-timer').classList.add('selected');
                selectedChoice = 'purple';
            }
            
            updateSelectionDisplay();
            updateBetAmount();
        }

        function selectNumber(number) {
            // Remove previous number selection
            document.querySelectorAll('.number-btn').forEach(btn => {
                btn.classList.remove('selected');
            });
            
            // Remove other selections
            document.querySelectorAll('.big-small-btn').forEach(btn => {
                btn.classList.remove('selected');
            });
            
            // Add selection to clicked number
            document.getElementById(num-${number}).classList.add('selected');
            selectedChoice = number-${number};
            
            updateSelectionDisplay();
            updateBetAmount();
        }

        function selectMultiplier(multiplier) {
            // Remove previous selection
            document.querySelectorAll('.multiplier-btn').forEach(btn => {
                btn.classList.remove('selected');
            });
            
            // Add selection to clicked button
            event.target.classList.add('selected');
            selectedMultiplier = multiplier;
            updateBetAmount();
        }

        function updateSelectionDisplay() {
            const container = document.getElementById('selection-display');
            if (!selectedChoice) {
                container.innerHTML = '<span class="text-gray-400">á€™á€›á€½á€±á€¸á€›á€á€±á€¸</span>';
            } else if (selectedChoice === 'big') {
                container.innerHTML = '<span class="text-red-600 font-bold">ğŸ”´ á€¡á€€á€¼á€®á€¸ (5, 6, 7, 8, 9)</span>';
            } else if (selectedChoice === 'small') {
                container.innerHTML = '<span class="text-blue-600 font-bold">ğŸ”µ á€¡á€á€±á€¸ (0, 1, 2, 3, 4)</span>';
            } else if (selectedChoice === 'red') {
                container.innerHTML = '<span class="text-red-600 font-bold">ğŸ’ á€¡á€”á€®á€›á€±á€¬á€„á€º (2, 4, 6, 8)</span>';
            } else if (selectedChoice === 'green') {
                container.innerHTML = '<span class="text-green-600 font-bold">ğŸ€ á€¡á€…á€­á€™á€ºá€¸á€›á€±á€¬á€„á€º (1, 3, 7, 9)</span>';
            } else if (selectedChoice === 'purple') {
                container.innerHTML = '<span class="text-purple-600 font-bold">ğŸ‘‘ á€á€›á€™á€ºá€¸á€›á€±á€¬á€„á€º (0, 5)</span>';

} else if (selectedChoice.startsWith('number-')) {
                const number = selectedChoice.split('-')[1];
                let numberColor = '';
                let numberIcon = '';
                
                // Determine color and icon based on number
                if ([2, 4, 6, 8].includes(parseInt(number))) {
                    numberColor = 'text-red-600';
                    numberIcon = 'ğŸ’';
                } else if ([1, 3, 7, 9].includes(parseInt(number))) {
                    numberColor = 'text-green-600';
                    numberIcon = 'ğŸ€';
                } else if ([0, 5].includes(parseInt(number))) {
                    numberColor = 'text-purple-600';
                    numberIcon = 'ğŸ‘‘';
                }
                
                container.innerHTML = <span class="${numberColor} font-bold">${numberIcon} á€”á€¶á€•á€«á€á€º ${number}</span>;
            }
        }

        function updateBetAmount() {
            const totalBet = selectedChoice ? baseBet * selectedMultiplier : 0;
            document.getElementById('bet-amount').textContent = ${totalBet.toLocaleString()} á€€á€»á€•á€º;
        }

        function confirmBet() {
            if (!selectedChoice) {
                alert('á€¡á€€á€¼á€®á€¸áŠ á€¡á€á€±á€¸áŠ á€¡á€”á€®á€›á€±á€¬á€„á€ºáŠ á€¡á€…á€­á€™á€ºá€¸á€›á€±á€¬á€„á€º á€á€­á€¯á€·á€™á€Ÿá€¯á€á€º á€á€›á€™á€ºá€¸á€›á€±á€¬á€„á€º á€›á€½á€±á€¸á€á€»á€šá€ºá€•á€«!');
                return;
            }
            
            const totalBet = baseBet * selectedMultiplier;
            let choiceText;
            if (selectedChoice === 'big') choiceText = 'á€¡á€€á€¼á€®á€¸';
            else if (selectedChoice === 'small') choiceText = 'á€¡á€á€±á€¸';
            else if (selectedChoice === 'red') choiceText = 'á€¡á€”á€®á€›á€±á€¬á€„á€º';
            else if (selectedChoice === 'green') choiceText = 'á€¡á€…á€­á€™á€ºá€¸á€›á€±á€¬á€„á€º';
            else if (selectedChoice === 'purple') choiceText = 'á€á€›á€™á€ºá€¸á€›á€±á€¬á€„á€º';
            
            // Show success message immediately without confirmation dialog
            showSuccessMessage(totalBet, choiceText);
        }

        function showSuccessMessage(betAmount, choice) {
            // Create large overlay success message
            const overlay = document.createElement('div');
            overlay.style.cssText = 
                position: fixed;
                top: 0;
                left: 0;
                width: 100%;
                height: 100%;
                background: rgba(0, 0, 0, 0.7);
                display: flex;
                align-items: center;
                justify-content: center;
                z-index: 99999;
                opacity: 0;
                transition: opacity 0.3s ease;
            ;
            
            // Create success icon container
            const successIcon = document.createElement('div');
            successIcon.style.cssText = 
                background: white;
                border-radius: 24px;
                padding: 32px;
                text-align: center;
                box-shadow: 0 25px 50px rgba(0, 0, 0, 0.25);
                transform: scale(0.5);
                transition: transform 0.5s ease;
                max-width: 320px;
                margin: 16px;
            ;
            
            successIcon.innerHTML = `
                <div style="font-size: 80px; margin-bottom: 16px; animation: bounce 1s infinite;">
                    <div style="position: relative; display: inline-block;">
                        <div style="color: #10b981;">âœ…</div>
                        <div style="position: absolute; top: -8px; left: 50%; transform: translateX(-50%); font-size: 24px; animation: pulse 2s infinite;">âœ¨</div>
                        <div style="position: absolute; top: -4px; left: 25%; transform: translateX(-50%); font-size: 20px; animation: pulse 2s infinite; animation-delay: 0.2s;">ğŸ’«</div>
                        <div style="position: absolute; top: -12px; right: 25%; transform: translateX(50%); font-size: 20px; animation: pulse 2s infinite; animation-delay: 0.4s;">â­</div>

</div>
                </div>
                <div style="font-size: 28px; font-weight: bold; color: #10b981; margin-bottom: 12px;">ğŸ‰ á€¡á€±á€¬á€„á€ºá€™á€¼á€„á€ºá€•á€«á€•á€¼á€®! ğŸ‰</div>
                <div style="font-size: 18px; color: #374151; margin-bottom: 8px;">á€œá€±á€¬á€„á€ºá€¸á€„á€½á€±: ${betAmount.toLocaleString()} á€€á€»á€•á€º</div>
                <div style="font-size: 18px; color: #374151; margin-bottom: 16px;">á€›á€½á€±á€¸á€á€»á€šá€ºá€™á€¾á€¯: ${choice}</div>
                <div style="font-size: 14px; color: #6b7280;">á€›á€œá€’á€ºá€€á€­á€¯ á€…á€±á€¬á€„á€·á€ºá€†á€­á€¯á€„á€ºá€¸á€•á€«...</div>
            ;
            
            overlay.appendChild(successIcon);
            document.body.appendChild(overlay);
            
            // Force reflow and animate in
            overlay.offsetHeight;
            overlay.style.opacity = '1';
            
            setTimeout(() => {
                successIcon.style.transform = 'scale(1)';
            }, 50);
            
            // Click to close
            overlay.addEventListener('click', () => {
                overlay.style.opacity = '0';
                setTimeout(() => {
                    if (document.body.contains(overlay)) {
                        document.body.removeChild(overlay);
                    }
                }, 300);
            });
            
            // Auto remove after 3 seconds
            setTimeout(() => {
                if (document.body.contains(overlay)) {
                    overlay.style.opacity = '0';
                    setTimeout(() => {
                        if (document.body.contains(overlay)) {
                            document.body.removeChild(overlay);
                        }
                    }, 300);
                }
            }, 3000);
        }

        function cancelBet() {
            // Clear all selections
            selectedChoice = null;
            selectedMultiplier = 1;
            
            // Reset UI
            document.querySelectorAll('.big-small-btn').forEach(btn => {
                btn.classList.remove('selected');
            });
            document.querySelectorAll('.multiplier-btn').forEach(btn => {
                btn.classList.remove('selected');
            });
            document.querySelectorAll('.number-btn').forEach(btn => {
                btn.classList.remove('selected');
            });
            
            updateSelectionDisplay();
            updateBetAmount();
            
            alert('âŒ á€œá€±á€¬á€„á€ºá€¸á€„á€½á€±á€•á€šá€ºá€–á€»á€€á€ºá€•á€¼á€®á€¸');
        }

        function simulateNewDraw() {
            // Generate random winning number (0-9)
            const winningNumber = Math.floor(Math.random() * 10);
            const isBig = winningNumber >= 5;
            const isRed = [2, 4, 6, 8].includes(winningNumber);
            const isGreen = [1, 3, 7, 9].includes(winningNumber);
            const isPurple = [0, 5].includes(winningNumber);
            
            // Determine ball color based on number
            let ballColor;
            if (isRed) ballColor = 'red';
            else if (isGreen) ballColor = 'green';
            else if (isPurple) ballColor = 'purple';
            else ballColor = isBig ? 'red' : 'blue';
            
            const resultText = isBig ? 'á€¡á€€á€¼á€®á€¸' : 'á€¡á€á€±á€¸';
            
            // Update the latest winning number display
            const ballsContainer = document.querySelector('.game-card .flex.justify-center.items-center.space-x-6');
            ballsContainer.innerHTML = 
                <div class="lottery-ball ${ballColor} w-16 h-16 rounded-full flex items-center justify-center text-2xl font-bold">${winningNumber}</div>
                <div class="flex flex-col items-center">
                    <div class="text-2xl font-bold ${isBig ? 'text-red-600' : 'text-blue-600'}">${resultText}</div>
                    <div class="text-sm text-gray-600">${isBig ? '(5-9)' : '(0-4)'}</div>

${isRed ? '<div class="text-sm text-red-600 font-bold">ğŸ’ á€¡á€”á€®á€›á€±á€¬á€„á€º</div>' : ''}
                    ${isGreen ? '<div class="text-sm text-green-600 font-bold">ğŸ€ á€¡á€…á€­á€™á€ºá€¸á€›á€±á€¬á€„á€º</div>' : ''}
                    ${isPurple ? '<div class="text-sm text-purple-600 font-bold">ğŸ‘‘ á€á€›á€™á€ºá€¸á€›á€±á€¬á€„á€º</div>' : ''}
                </div>
            ;
            
            // Update round number
            const roundElement = document.querySelector('.game-card h2 span');
            const currentRound = parseInt(roundElement.textContent.replace('#á€¡á€€á€¼á€­á€™á€º ', ''));
            roundElement.textContent = #á€¡á€€á€¼á€­á€™á€º ${currentRound + 1};
            
            // Update timer bar round number
            const timerRoundElement = document.getElementById('round-number');
            timerRoundElement.textContent = #${currentRound + 1};
            
            // Update recent numbers in timer bar
            updateRecentNumbers(winningNumber, ballColor);
            
            // Check win/lose
            if (selectedChoice) {
                let isWin = false;
                let winMultiplier = 1.95; // Default payout
                
                if (selectedChoice === 'big' && isBig) isWin = true;
                else if (selectedChoice === 'small' && !isBig) isWin = true;
                else if (selectedChoice === 'red' && isRed) {
                    isWin = true;
                    winMultiplier = 3.5; // Higher payout for red color bets
                }
                else if (selectedChoice === 'green' && isGreen) {
                    isWin = true;
                    winMultiplier = 3.5; // Higher payout for green color bets
                }
                else if (selectedChoice === 'purple' && isPurple) {
                    isWin = true;
                    winMultiplier = 6.0; // Highest payout for purple (only 2 numbers)
                }
                else if (selectedChoice.startsWith('number-')) {
                    const selectedNumber = parseInt(selectedChoice.split('-')[1]);
                    if (selectedNumber === winningNumber) {
                        isWin = true;
                        winMultiplier = 9.0; // Highest payout for exact number match
                    }
                }
                
                if (isWin) {
                    const winAmount = baseBet * selectedMultiplier * winMultiplier;
                    showWinLoseIcon('win', winAmount, winningNumber, resultText);
                } else {
                    showWinLoseIcon('lose', 0, winningNumber, resultText);
                }
            } else {
                // Just show the result without win/lose
                setTimeout(() => {
                    alert(ğŸŠ á€¡á€á€…á€ºá€‘á€½á€€á€ºá€•á€¼á€®á€¸! á€”á€¶á€•á€«á€á€º: ${winningNumber} (${resultText}));
                }, 500);
            }
        }

        function updateRecentNumbers(newNumber, ballColor) {
            const recentContainer = document.getElementById('recent-numbers');
            let bgColor;
            
            // Set background color based on ball color
            switch(ballColor) {
                case 'green': bgColor = 'bg-green-600'; break;
                case 'purple': bgColor = 'bg-purple-600'; break;
                case 'red': bgColor = 'bg-red-600'; break;
                case 'blue': bgColor = 'bg-blue-600'; break;
                default: bgColor = 'bg-gray-600';
            }
            
            // Create new number element
            const newNumberElement = <div class="w-5 h-5 ${bgColor} rounded-full flex items-center justify-center text-xs font-bold text-white">${newNumber}</div>`;
            
            // Get current numbers and add new one at the beginning
            const currentNumbers = recentContainer.children;
            const numbersArray = Array.from(currentNumbers).map(el => el.outerHTML);

// Keep only the last 4 numbers and add the new one at the beginning
            numbersArray.unshift(newNumberElement);
            if (numbersArray.length > 5) {
                numbersArray.pop();
            }
            
            // Update the container
            recentContainer.innerHTML = numbersArray.join('');
        }

        function showWinLoseIcon(result, winAmount, winningNumber, resultText) {
            // Create overlay
            const overlay = document.createElement('div');
            overlay.className = 'fixed inset-0 bg-black bg-opacity-70 flex items-center justify-center z-50';
            overlay.style.zIndex = '9999';
            
            // Create icon container
            const iconContainer = document.createElement('div');
            iconContainer.className = 'bg-white rounded-3xl p-12 text-center shadow-2xl transform scale-0 transition-all duration-700 max-w-sm mx-4';
            
            if (result === 'win') {
                iconContainer.innerHTML = 
                    <div class="text-9xl mb-6 relative animate-bounce">
                        <div class="relative inline-block">
                            <div class="text-red-500">ğŸ”´</div>
                            <div class="absolute -top-4 left-1/2 transform -translate-x-1/2 text-3xl animate-pulse">âœ¨</div>
                            <div class="absolute -top-2 left-1/4 transform -translate-x-1/2 text-2xl animate-pulse" style="animation-delay: 0.2s">ğŸ’«</div>
                            <div class="absolute -top-6 right-1/4 transform translate-x-1/2 text-2xl animate-pulse" style="animation-delay: 0.4s">â­</div>
                            <div class="absolute -top-3 right-1/2 transform translate-x-1/2 text-xl animate-pulse" style="animation-delay: 0.6s">âœ¨</div>
                        </div>
                    </div>
                    <div class="text-4xl font-bold text-red-600 mb-4">ğŸ‰ á€”á€­á€¯á€„á€ºá€•á€¼á€®á€¸! ğŸ‰</div>
                    <div class="text-xl text-gray-700 mb-2">á€”á€¶á€•á€«á€á€º: ${winningNumber} (${resultText})</div>
                    <div class="text-2xl font-bold text-green-600">${winAmount.toLocaleString()} á€€á€»á€•á€º á€›á€›á€¾á€­!</div>
                    <div class="text-lg text-gray-600 mt-2">á€€á€¶á€€á€±á€¬á€„á€ºá€¸á€•á€«á€á€šá€º! ğŸ€</div>
                ;
            } else {
                iconContainer.innerHTML = 
                    <div class="text-9xl mb-6">
                        <div class="relative inline-block">
                            <div class="text-gray-700">âš«</div>
                            <div class="absolute top-1/2 left-1/2 transform -translate-x-1/2 -translate-y-1/2 text-4xl">ğŸ˜”</div>
                        </div>
                    </div>
                    <div class="text-4xl font-bold text-gray-700 mb-4">ğŸ˜ á€›á€¾á€¯á€¶á€¸á€•á€¼á€®á€¸! ğŸ˜</div>
                    <div class="text-xl text-gray-600 mb-2">á€”á€¶á€•á€«á€á€º: ${winningNumber} (${resultText})</div>
                    <div class="text-lg text-gray-500 mt-2">á€”á€±á€¬á€€á€ºá€á€…á€ºá€€á€¼á€­á€™á€º á€€á€¶á€€á€±á€¬á€„á€ºá€¸á€•á€«á€…á€±! ğŸ™</div>
                ;
            }
            
            overlay.appendChild(iconContainer);
            document.body.appendChild(overlay);
            
            // Animate in
            setTimeout(() => {
                iconContainer.classList.remove('scale-0');
                iconContainer.classList.add('scale-100');
            }, 200);
            
            // Click to close
            overlay.addEventListener('click', () => {
                overlay.classList.add('opacity-0');
                setTimeout(() => {
                    if (document.body.contains(overlay)) {
                        document.body.removeChild(overlay);
                    }
                }, 300);
            });
            
            // Auto remove after 4 seconds
            setTimeout(() => {

if (document.body.contains(overlay)) {
                    overlay.classList.add('opacity-0');
                    setTimeout(() => {
                        if (document.body.contains(overlay)) {
                            document.body.removeChild(overlay);
                        }
                    }, 300);
                }
            }, 4000);
        }

        function showDepositModal() {
            const amount = prompt('á€„á€½á€±á€á€½á€„á€ºá€¸á€™á€Šá€·á€ºá€•á€™á€¬á€ (Ks):');
            if (amount && !isNaN(amount) && parseInt(amount) > 0) {
                const currentBalance = getCurrentBalance();
                const newBalance = currentBalance + parseInt(amount);
                updateBalance(newBalance);
                alert(âœ… ${parseInt(amount).toLocaleString()} Ks á€„á€½á€±á€á€½á€„á€ºá€¸á€•á€¼á€®á€¸á€•á€«á€•á€¼á€®!);
            }
        }

        function showWithdrawModal() {
            const currentBalance = getCurrentBalance();
            const amount = prompt(á€„á€½á€±á€‘á€¯á€á€ºá€™á€Šá€·á€ºá€•á€™á€¬á€ (Ks):\ná€œá€€á€ºá€€á€»á€”á€ºá€„á€½á€±: ${currentBalance.toLocaleString()} Ks);
            if (amount && !isNaN(amount) && parseInt(amount) > 0) {
                if (parseInt(amount) <= currentBalance) {
                    const newBalance = currentBalance - parseInt(amount);
                    updateBalance(newBalance);
                    alert(âœ… ${parseInt(amount).toLocaleString()} Ks á€„á€½á€±á€‘á€¯á€á€ºá€•á€¼á€®á€¸á€•á€«á€•á€¼á€®!);
                } else {
                    alert('âŒ á€œá€€á€ºá€€á€»á€”á€ºá€„á€½á€± á€™á€œá€¯á€¶á€œá€±á€¬á€€á€ºá€•á€«!');
                }
            }
        }

        function getCurrentBalance() {
            const balanceText = document.getElementById('balance-display').textContent;
            const balanceNumber = balanceText.replace(/[^\d]/g, '');
            return parseInt(balanceNumber) || 125430;
        }

        function updateBalance(newBalance) {
            document.getElementById('balance-display').textContent = ğŸ’° ${newBalance.toLocaleString()} Ks;
        }

        function selectWingoGame(gameType) {
            // Remove previous selection
            document.querySelectorAll('.wingo-btn').forEach(btn => {
                btn.classList.remove('bg-white', 'bg-opacity-30', 'font-bold');
                btn.classList.add('bg-white', 'bg-opacity-15');
            });
            
            // Add selection to clicked button
            const selectedBtn = document.getElementById(wingo-${gameType});
            selectedBtn.classList.remove('bg-opacity-15');
            selectedBtn.classList.add('bg-opacity-30', 'font-bold');
            
            // Update timer based on selection
            let timerSeconds;
            switch(gameType) {
                case '30s': timerSeconds = 30; break;
                case '1min': timerSeconds = 60; break;
                case '3min': timerSeconds = 180; break;
                case '5min': timerSeconds = 300; break;
                default: timerSeconds = 30;
            }
            
            countdownTimer = timerSeconds;
            updateTimerDisplay();
            
            // Show selection feedback
            const gameNames = {
                '30s': 'Wingo 30s âš¡',
                '1min': 'Win Go 1min ğŸ¯', 
                '3min': 'Wingo 3min ğŸ²',
                '5min': 'Wingo 5min ğŸ†'
            };
            
            // Update current game type display
            document.getElementById('current-game-type').textContent = gameNames[gameType];
            
            // Update timer text
            updateTimerDisplay();
        }

        function quickBet(amount) {
            if (!selectedChoice) {
                alert('á€•á€‘á€™á€¦á€¸á€…á€½á€¬ á€¡á€€á€¼á€®á€¸áŠ á€¡á€á€±á€¸áŠ á€¡á€›á€±á€¬á€„á€º á€á€­á€¯á€·á€™á€Ÿá€¯á€á€º á€”á€¶á€•á€«á€á€º á€›á€½á€±á€¸á€á€»á€šá€ºá€•á€«!');
                return;
            }
            
            // Set the bet amount directly
            baseBet = amount;
            selectedMultiplier = 1; // Reset multiplier to 1 since we're setting direct amount

// Update multiplier buttons to show none selected
            document.querySelectorAll('.multiplier-btn').forEach(btn => {
                btn.classList.remove('selected');
            });
            
            // Update bet amount display
            updateBetAmount();
            
            // Show quick bet confirmation
            let choiceText;
            if (selectedChoice === 'big') choiceText = 'á€¡á€€á€¼á€®á€¸';
            else if (selectedChoice === 'small') choiceText = 'á€¡á€á€±á€¸';
            else if (selectedChoice === 'red') choiceText = 'á€¡á€”á€®á€›á€±á€¬á€„á€º';
            else if (selectedChoice === 'green') choiceText = 'á€¡á€…á€­á€™á€ºá€¸á€›á€±á€¬á€„á€º';
            else if (selectedChoice === 'purple') choiceText = 'á€á€›á€™á€ºá€¸á€›á€±á€¬á€„á€º';
            else if (selectedChoice.startsWith('number-')) {
                const number = selectedChoice.split('-')[1];
                choiceText = á€”á€¶á€•á€«á€á€º ${number};
            }
            
            // Auto-confirm the bet
            showSuccessMessage(amount, choiceText);
        }

        // Initialize the game
        startTimer();
        updateBetAmount();
        
        // Auto-select first multiplier and Wingo game
        document.querySelector('.multiplier-btn').classList.add('selected');
        selectWingoGame('30s');
    </script>
