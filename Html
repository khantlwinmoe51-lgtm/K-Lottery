        <!-- Results History -->
        <div class="game-card rounded-2xl p-6">
            <h3 class="text-lg font-bold text-gray-800 mb-4">ğŸ“Š á€™á€€á€¼á€¬á€á€±á€¸á€™á€®á€€á€›á€œá€’á€ºá€™á€»á€¬á€¸</h3>
            <div class="space-y-3">
                <div class="flex items-center justify-between p-3 bg-gray-50 rounded-xl">
                    <div class="flex items-center space-x-2">
                        <span class="text-sm text-gray-600">#1246</span>
